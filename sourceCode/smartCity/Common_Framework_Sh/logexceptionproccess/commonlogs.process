<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/smatcity/SmartCity_Schema/CommonModel/LogException/Schema.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Schema/CommonModel/LogException/Schema.xsd" schemaLocation="/Common_Framework_Sh/Exception_schema/Exception.xsd"/>
    <pd:name>Common_Framework_Sh/logexceptionproccess/commonlogs.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:LogHandler"/>
    <pd:startX>149</pd:startX>
    <pd:startY>70</pd:startY>
    <pd:returnBindings>
        <pfx:LogHandler>
            <xsl:if test="$Start/pfx:LogHandler/pfx:ApplictionName">
                <pfx:ApplictionName>
                    <xsl:value-of select="$Start/pfx:LogHandler/pfx:ApplictionName"/>
                </pfx:ApplictionName>
            </xsl:if>
            <xsl:if test="$Start/pfx:LogHandler/pfx:ProcessName">
                <pfx:ProcessName>
                    <xsl:value-of select="$Start/pfx:LogHandler/pfx:ProcessName"/>
                </pfx:ProcessName>
            </xsl:if>
            <xsl:if test="$Start/pfx:LogHandler/pfx:CorrelationID">
                <pfx:CorrelationID>
                    <xsl:value-of select="$Start/pfx:LogHandler/pfx:CorrelationID"/>
                </pfx:CorrelationID>
            </xsl:if>
            <xsl:if test="$Start/pfx:LogHandler/pfx:Status">
                <pfx:Status>
                    <xsl:value-of select="$Start/pfx:LogHandler/pfx:Status"/>
                </pfx:Status>
            </xsl:if>
            <xsl:if test="$Start/pfx:LogHandler/pfx:LogType">
                <pfx:LogType>
                    <xsl:value-of select="$Start/pfx:LogHandler/pfx:LogType"/>
                </pfx:LogType>
            </xsl:if>
            <xsl:if test="$Start/pfx:LogHandler/pfx:Current_Timestamp">
                <pfx:Current_Timestamp>
                    <xsl:value-of select="$Start/pfx:LogHandler/pfx:Current_Timestamp"/>
                </pfx:Current_Timestamp>
            </xsl:if>
            <xsl:if test="$Start/pfx:LogHandler/pfx:Payload">
                <pfx:Payload>
                    <xsl:value-of select="$Start/pfx:LogHandler/pfx:Payload"/>
                </pfx:Payload>
            </xsl:if>
        </pfx:LogHandler>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:LogHandler"/>
    <pd:endX>445</pd:endX>
    <pd:endY>70</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1679549107602</pd:targetNamespace>
    <pd:activity name="commonlogs">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>293</pd:x>
        <pd:y>72</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>INSERT INTO SYSTEM.SCP_LOGEXCEPTION (APPLICATIONNAME, PROCESSNAME,
  CORRELATIONID, STATUS, LOGTYPE, C_TIMESTAMP,
  PAYLOAD)
  VALUES (?, ?, ?, ?, ?, ?, ?)</statement>
            <jdbcSharedConfig>/SmartCity_Project/CommonModel/Resource/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>APPLICATIONNAME</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PROCESSNAME</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>CORRELATIONID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>STATUS</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>LOGTYPE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>C_TIMESTAMP</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PAYLOAD</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <APPLICATIONNAME>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:LogHandler/pfx:ApplictionName)">
                            <xsl:value-of select="$Start/pfx:LogHandler/pfx:ApplictionName"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </APPLICATIONNAME>
                <PROCESSNAME>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:LogHandler/pfx:ProcessName)">
                            <xsl:value-of select="$Start/pfx:LogHandler/pfx:ProcessName"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </PROCESSNAME>
                <CORRELATIONID>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:LogHandler/pfx:CorrelationID)">
                            <xsl:value-of select="$Start/pfx:LogHandler/pfx:CorrelationID"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </CORRELATIONID>
                <STATUS>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:LogHandler/pfx:Status)">
                            <xsl:value-of select="$Start/pfx:LogHandler/pfx:Status"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </STATUS>
                <LOGTYPE>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:LogHandler/pfx:LogType)">
                            <xsl:value-of select="$Start/pfx:LogHandler/pfx:LogType"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </LOGTYPE>
                <C_TIMESTAMP>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:LogHandler/pfx:Current_Timestamp)">
                            <xsl:value-of select="$Start/pfx:LogHandler/pfx:Current_Timestamp"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </C_TIMESTAMP>
                <PAYLOAD>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:LogHandler/pfx:Payload)">
                            <xsl:value-of select="$Start/pfx:LogHandler/pfx:Payload"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </PAYLOAD>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>commonlogs</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>commonlogs</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>