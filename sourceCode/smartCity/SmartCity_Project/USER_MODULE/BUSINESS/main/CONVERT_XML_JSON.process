<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx3="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/BusinessSchema/Schema.xsd" xmlns:pfx2="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/common/Schema.xsd" xmlns:pfx="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/UserSchema/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/UserSchema/Schema.xsd" schemaLocation="/SmartCity_Project/CommonModel/UserSchema/UserRequest.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/common/Schema.xsd" schemaLocation="/SmartCity_Project/CommonModel/AdminSchema/common/xml_json.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/BusinessSchema/Schema.xsd" schemaLocation="/SmartCity_Project/CommonModel/AdminSchema/BusinessSchema/BusinessResp.xsd"/>
    <pd:name>SmartCity_Project/USER_MODULE/BUSINESS/main/CONVERT_XML_JSON.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx2:convert"/>
    <pd:startX>112</pd:startX>
    <pd:startY>110</pd:startY>
    <pd:returnBindings>
        <payload>
            <xsl:value-of select="$Render-JSON/ns:ActivityOutputClass/jsonString"/>
        </payload>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="payload" type="xsd:string"/>
    </pd:endType>
    <pd:endX>630</pd:endX>
    <pd:endY>108</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1681737081671</pd:targetNamespace>
    <pd:activity name="Parse JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONParserActivity</pd:type>
        <pd:resourceType>ae.activities.JSONParserActivity</pd:resourceType>
        <pd:x>240</pd:x>
        <pd:y>110</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityOutputEditor ref="pfx:UserReq"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <jsonString>
                    <xsl:value-of select="$Start/pfx2:convert/pfx2:payload"/>
                </jsonString>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>489</pd:x>
        <pd:y>109</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <removeRootElementForInput>true</removeRootElementForInput>
            <ActivityInputEditor ref="pfx3:BusinessResp"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <pfx3:BusinessResp>
                    <pfx3:Status>
                        <xsl:value-of select="$Get_business_details/pfx3:BusinessResp/pfx3:Status"/>
                    </pfx3:Status>
                    <pfx3:message>
                        <xsl:value-of select="$Get_business_details/pfx3:BusinessResp/pfx3:message"/>
                    </pfx3:message>
                    <pfx3:BusinessInfo>
                        <xsl:for-each select="$Get_business_details/pfx3:BusinessResp/pfx3:BusinessInfo/pfx3:Industries">
                            <pfx3:Industries>
                                <pfx3:Industry_Id>
                                    <xsl:value-of select="pfx3:Industry_Id"/>
                                </pfx3:Industry_Id>
                                <pfx3:Industry_name>
                                    <xsl:value-of select="pfx3:Industry_name"/>
                                </pfx3:Industry_name>
                                <pfx3:Descriptions>
                                    <xsl:value-of select="pfx3:Descriptions"/>
                                </pfx3:Descriptions>
                                <pfx3:Locations>
                                    <xsl:value-of select="pfx3:Locations"/>
                                </pfx3:Locations>
                                <pfx3:Contact_email>
                                    <xsl:value-of select="pfx3:Contact_email"/>
                                </pfx3:Contact_email>
                            </pfx3:Industries>
                        </xsl:for-each>
                    </pfx3:BusinessInfo>
                </pfx3:BusinessResp>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get_business_details">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>365</pd:x>
        <pd:y>111</pd:y>
        <config>
            <processName>/SmartCity_Project/USER_MODULE/BUSINESS/main/Get_business_details.process</processName>
        </config>
        <pd:inputBindings>
            <pfx:UserReq>
                <pfx:Search_criteria>
                    <xsl:value-of select="$Parse-JSON/ns:ActivityOutputClass/pfx:UserReq/pfx:Search_criteria"/>
                </pfx:Search_criteria>
            </pfx:UserReq>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Parse JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render JSON</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse JSON</pd:from>
        <pd:to>Get_business_details</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get_business_details</pd:from>
        <pd:to>Render JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>