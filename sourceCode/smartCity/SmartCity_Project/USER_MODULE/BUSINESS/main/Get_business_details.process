        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/pfx:UserReq/pfx:Search_criteria = 'Business_news'</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>startlog</pd:from>
        <pd:to>Business_centers</pd:to>
