        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>411</pd:x>
        <pd:y>210</pd:y>
        <config>
            <processName>/SmartCity_Project/USER_MODULE/BUSINESS/main/subprocess/Industries.process</processName>
        </config>
        <pd:inputBindings>
            <pfx:UserReq>
                <pfx:Search_criteria>
                    <xsl:value-of select="$Start/pfx:UserReq/pfx:Search_criteria"/>
                </pfx:Search_criteria>
            </pfx:UserReq>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="startlog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>252</pd:x>
        <pd:y>124</pd:y>
        <config>
            <processName>/Common_Framework_Sh/logexceptionproccess/commonlogs.process</processName>
        </config>
        <pd:inputBindings>
            <ns:LogHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="&quot;business_details&quot;"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="&quot;Get_business_details&quot;"/>
                </ns:ProcessName>
                <ns:CorrelationID>
                    <xsl:value-of select="$Correlatn_Id/Correlatn_Id"/>
                </ns:CorrelationID>
                <ns:Status>
                    <xsl:value-of select="&quot;success&quot;"/>
                </ns:Status>
                <ns:LogType>
                    <xsl:value-of select="&quot;startlog&quot;"/>
                </ns:LogType>
                <ns:Current_Timestamp>
                    <xsl:value-of select="current-dateTime()"/>
                </ns:Current_Timestamp>
                <ns:Payload>
                    <xsl:value-of select="string($Start/pfx:UserReq)"/>
                </ns:Payload>
            </ns:LogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="end_log">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>554</pd:x>
        <pd:y>125</pd:y>
        <config>
            <processName>/Common_Framework_Sh/logexceptionproccess/commonlogs.process</processName>
        </config>
        <pd:inputBindings>
            <ns:LogHandler>
                <xsl:if test="$startlog/ns:LogHandler/ns:ApplictionName">
                    <ns:ApplictionName>
                        <xsl:value-of select="$startlog/ns:LogHandler/ns:ApplictionName"/>
                    </ns:ApplictionName>
                </xsl:if>
                <xsl:if test="$startlog/ns:LogHandler/ns:ProcessName">
                    <ns:ProcessName>
                        <xsl:value-of select="$startlog/ns:LogHandler/ns:ProcessName"/>
                    </ns:ProcessName>
                </xsl:if>
                <xsl:if test="$startlog/ns:LogHandler/ns:CorrelationID">
                    <ns:CorrelationID>
                        <xsl:value-of select="$startlog/ns:LogHandler/ns:CorrelationID"/>
                    </ns:CorrelationID>
                </xsl:if>
                <xsl:if test="$startlog/ns:LogHandler/ns:Status">
                    <ns:Status>
                        <xsl:value-of select="$startlog/ns:LogHandler/ns:Status"/>
                    </ns:Status>
                </xsl:if>
                <ns:LogType>
                    <xsl:value-of select="&quot;endlog&quot;"/>
                </ns:LogType>
                <xsl:if test="$startlog/ns:LogHandler/ns:Current_Timestamp">
                    <ns:Current_Timestamp>
                        <xsl:value-of select="$startlog/ns:LogHandler/ns:Current_Timestamp"/>
                    </ns:Current_Timestamp>
                </xsl:if>
                <xsl:if test="$startlog/ns:LogHandler/ns:Payload">
                    <ns:Payload>
                        <xsl:value-of select="$startlog/ns:LogHandler/ns:Payload"/>
                    </ns:Payload>
                </xsl:if>
            </ns:LogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>169</pd:x>
        <pd:y>123</pd:y>
        <config>
            <variableName>Correlatn_Id</variableName>
        </config>
        <pd:inputBindings>
            <Correlatn_Id>
                <xsl:value-of select="custom_func:getUUID()"/>
            </Correlatn_Id>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Assign</pd:to>
        <pd:xpathDescription/>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign</pd:from>
        <pd:to>startlog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
