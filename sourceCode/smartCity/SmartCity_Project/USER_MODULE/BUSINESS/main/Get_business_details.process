        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/pfx:UserReq/pfx:Search_criteria ='Business_centers'</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>startlog</pd:from>
        <pd:to>Industries</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/pfx:UserReq/pfx:Search_criteria = 'Industries'</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Business_centers</pd:from>
        <pd:to>end_log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Business_news</pd:from>
        <pd:to>end_log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Industries</pd:from>
        <pd:to>end_log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>end_log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>