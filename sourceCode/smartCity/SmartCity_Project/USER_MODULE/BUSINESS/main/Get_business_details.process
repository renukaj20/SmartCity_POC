<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/BusinessSchema/SubprocessSchema/Schema.xsd2" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/BusinessSchema/SubprocessSchema/Schema.xsd" xmlns:ns="http://www.tibco.com/schemas/smatcity/SmartCity_Schema/CommonModel/LogException/Schema.xsd" xmlns:ns3="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/BusinessSchema/SubprocessSchema/Schema1.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx2="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/BusinessSchema/Schema.xsd" xmlns:custom_func="java://Custom.Custom" xmlns:pfx="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/UserSchema/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/UserSchema/Schema.xsd" schemaLocation="/SmartCity_Project/CommonModel/UserSchema/UserRequest.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/BusinessSchema/Schema.xsd" schemaLocation="/SmartCity_Project/CommonModel/AdminSchema/BusinessSchema/BusinessResp.xsd"/>
    <pd:name>SmartCity_Project/USER_MODULE/BUSINESS/main/Get_business_details.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:UserReq"/>
    <pd:startX>66</pd:startX>
    <pd:startY>124</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="$Start/pfx:UserReq/pfx:Search_criteria = 'Business_news'">
                <pfx2:BusinessResp>
                    <pfx2:Status>
                        <xsl:value-of select="$Business_news/ns1:NewsResp/ns1:status"/>
                    </pfx2:Status>
                    <pfx2:message>
                        <xsl:value-of select="$Business_news/ns1:NewsResp/ns1:message"/>
                    </pfx2:message>
                    <pfx2:BusinessInfo>
                        <xsl:for-each select="$Business_news/ns1:NewsResp/ns1:NewsInfo">
                            <pfx2:News>
                                <pfx2:Title>
                                    <xsl:value-of select="ns1:Title"/>
                                </pfx2:Title>
                                <pfx2:Description>
                                    <xsl:value-of select="ns1:Description"/>
                                </pfx2:Description>
                                <pfx2:Published_Date>
                                    <xsl:value-of select="ns1:Published_Date"/>
                                </pfx2:Published_Date>
                                <pfx2:Source>
                                    <xsl:value-of select="ns1:Source"/>
                                </pfx2:Source>
                            </pfx2:News>
                        </xsl:for-each>
                    </pfx2:BusinessInfo>
                </pfx2:BusinessResp>
            </xsl:when>
            <xsl:when test="$Start/pfx:UserReq/pfx:Search_criteria = 'Business_centers'">
                <pfx2:BusinessResp>
                    <pfx2:Status>
                        <xsl:value-of select="$Business_centers/ns2:Business_centersResp/ns2:status"/>
                    </pfx2:Status>
                    <pfx2:message>
                        <xsl:value-of select="$Business_centers/ns2:Business_centersResp/ns2:message"/>
                    </pfx2:message>
                    <pfx2:BusinessInfo>
                        <xsl:for-each select="$Business_centers/ns2:Business_centersResp/ns2:Business_centersInfo">
                            <pfx2:Business_centers>
                                <pfx2:Center_name>
                                    <xsl:value-of select="ns2:Center_name"/>
                                </pfx2:Center_name>
                                <pfx2:Locations>
                                    <xsl:value-of select="ns2:Locations"/>
                                </pfx2:Locations>
                                <pfx2:Descriptions>
                                    <xsl:value-of select="ns2:Descriptions"/>
                                </pfx2:Descriptions>
                                <pfx2:Contact_email>
                                    <xsl:value-of select="ns2:Contact_email"/>
                                </pfx2:Contact_email>
                            </pfx2:Business_centers>
                        </xsl:for-each>
                    </pfx2:BusinessInfo>
                </pfx2:BusinessResp>
            </xsl:when>
            <xsl:when test="$Start/pfx:UserReq/pfx:Search_criteria= 'Industries'">
                <pfx2:BusinessResp>
                    <pfx2:Status>
                        <xsl:value-of select="$Industries/ns3:IndustriesResp/ns3:status"/>
                    </pfx2:Status>
                    <pfx2:message>
                        <xsl:value-of select="$Industries/ns3:IndustriesResp/ns3:message"/>
                    </pfx2:message>
                    <pfx2:BusinessInfo>
                        <xsl:for-each select="$Industries/ns3:IndustriesResp/ns3:IndustriesInfo">
                            <pfx2:Industries>
                                <pfx2:Industry_Id>
                                    <xsl:value-of select="ns3:Industry_Id"/>
                                </pfx2:Industry_Id>
                                <pfx2:Industry_name>
                                    <xsl:value-of select="ns3:Industry_name"/>
                                </pfx2:Industry_name>
                                <pfx2:Descriptions>
                                    <xsl:value-of select="ns3:Descriptions"/>
                                </pfx2:Descriptions>
                                <pfx2:Locations>
                                    <xsl:value-of select="ns3:Locations"/>
                                </pfx2:Locations>
                                <pfx2:Contact_email>
                                    <xsl:value-of select="ns3:Contact_email"/>
                                </pfx2:Contact_email>
                            </pfx2:Industries>
                        </xsl:for-each>
                    </pfx2:BusinessInfo>
                </pfx2:BusinessResp>
            </xsl:when>
            <xsl:otherwise>
                <pfx2:BusinessResp>
                    <pfx2:Status>
                        <xsl:value-of select="&quot;FAILED&quot;"/>
                    </pfx2:Status>
                    <pfx2:message>
                        <xsl:value-of select="&quot;Failed to fetch the records&quot;"/>
                    </pfx2:message>
                </pfx2:BusinessResp>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:BusinessResp"/>
    <pd:endX>667</pd:endX>
    <pd:endY>127</pd:endY>
