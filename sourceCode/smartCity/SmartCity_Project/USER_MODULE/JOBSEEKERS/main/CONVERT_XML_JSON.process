<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx3="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/JobseekersSchema/Schema.xsd" xmlns:pfx2="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/UserSchema/Schema.xsd" xmlns:pfx="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/common/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/common/Schema.xsd" schemaLocation="/SmartCity_Project/CommonModel/AdminSchema/common/xml_json.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/UserSchema/Schema.xsd" schemaLocation="/SmartCity_Project/CommonModel/UserSchema/UserRequest.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/JobseekersSchema/Schema.xsd" schemaLocation="/SmartCity_Project/CommonModel/AdminSchema/JobseekersSchema/JobsseekersReq.xsd"/>
    <pd:name>SmartCity_Project/USER_MODULE/JOBSEEKERS/main/CONVERT_XML_JSON.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:convert"/>
    <pd:startX>112</pd:startX>
    <pd:startY>110</pd:startY>
    <pd:returnBindings>
        <payload>
            <xsl:value-of select="$Render-JSON/ns:ActivityOutputClass/jsonString"/>
        </payload>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="payload" type="xsd:string"/>
    </pd:endType>
    <pd:endX>630</pd:endX>
    <pd:endY>108</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1681737081671</pd:targetNamespace>
    <pd:activity name="Parse JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONParserActivity</pd:type>
        <pd:resourceType>ae.activities.JSONParserActivity</pd:resourceType>
        <pd:x>239</pd:x>
        <pd:y>111</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ValidateOutputEnable>true</ValidateOutputEnable>
            <ActivityOutputEditor ref="pfx2:UserReq"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <jsonString>
                    <xsl:value-of select="$Start/pfx:convert/pfx:payload"/>
                </jsonString>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>489</pd:x>
        <pd:y>109</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <removeRootElementForInput>true</removeRootElementForInput>
            <ActivityInputEditor ref="pfx3:JobseekersResp"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <pfx3:JobseekersResp>
                    <pfx3:Status>
                        <xsl:value-of select="$Get_jobseekers_details/pfx3:JobseekersResp/pfx3:Status"/>
                    </pfx3:Status>
                    <pfx3:message>
                        <xsl:value-of select="$Get_jobseekers_details/pfx3:JobseekersResp/pfx3:message"/>
                    </pfx3:message>
                    <xsl:for-each select="$Get_jobseekers_details/pfx3:JobseekersResp/pfx3:JobseekersInfo">
                        <pfx3:JobseekersInfo>
                            <pfx3:JobId>
                                <xsl:value-of select="pfx3:JobId"/>
                            </pfx3:JobId>
                            <pfx3:Role>
                                <xsl:value-of select="pfx3:Role"/>
                            </pfx3:Role>
                            <pfx3:postedDate>
                                <xsl:value-of select="pfx3:postedDate"/>
                            </pfx3:postedDate>
                            <pfx3:Company>
                                <xsl:value-of select="pfx3:Company"/>
                            </pfx3:Company>
                            <pfx3:Industries>
                                <xsl:value-of select="pfx3:Industries"/>
                            </pfx3:Industries>
                            <pfx3:location>
                                <xsl:value-of select="pfx3:location"/>
                            </pfx3:location>
                            <pfx3:Experience>
                                <xsl:value-of select="pfx3:Experience"/>
                            </pfx3:Experience>
                            <pfx3:Job_Type>
                                <xsl:value-of select="pfx3:Job_Type"/>
                            </pfx3:Job_Type>
                            <pfx3:Job_description>
                                <xsl:value-of select="pfx3:Job_description"/>
                            </pfx3:Job_description>
                            <pfx3:Requirements>
                                <pfx3:degree>
                                    <xsl:value-of select="pfx3:Requirements/pfx3:degree"/>
                                </pfx3:degree>
                                <pfx3:skills>
                                    <xsl:value-of select="pfx3:Requirements/pfx3:skills"/>
                                </pfx3:skills>
                                <pfx3:year_of_exp>
                                    <xsl:value-of select="pfx3:Requirements/pfx3:year_of_exp"/>
                                </pfx3:year_of_exp>
                            </pfx3:Requirements>
                            <pfx3:Contact_no>
                                <pfx3:name>
                                    <xsl:value-of select="pfx3:Contact_no/pfx3:name"/>
                                </pfx3:name>
                                <pfx3:email>
                                    <xsl:value-of select="pfx3:Contact_no/pfx3:email"/>
                                </pfx3:email>
                                <pfx3:phone>
                                    <xsl:value-of select="pfx3:Contact_no/pfx3:phone"/>
                                </pfx3:phone>
                            </pfx3:Contact_no>
                        </pfx3:JobseekersInfo>
                    </xsl:for-each>
                </pfx3:JobseekersResp>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get_jobseekers_details">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>366</pd:x>
        <pd:y>109</pd:y>
        <config>
            <processName>/SmartCity_Project/USER_MODULE/JOBSEEKERS/main/Get_jobseekers_details.process</processName>
        </config>
        <pd:inputBindings>
            <pfx2:UserReq>
                <pfx2:Search_criteria>
                    <xsl:value-of select="$Parse-JSON/ns:ActivityOutputClass/pfx2:UserReq/pfx2:Search_criteria"/>
                </pfx2:Search_criteria>
            </pfx2:UserReq>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Parse JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render JSON</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse JSON</pd:from>
        <pd:to>Get_jobseekers_details</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get_jobseekers_details</pd:from>
        <pd:to>Render JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>