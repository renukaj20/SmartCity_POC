<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/schemas/smatcity/SmartCity_Schema/CommonModel/LogException/Schema.xsd" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx2="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/JobseekersSchema/Schema.xsd" xmlns:custom_func="java://Custom.Custom" xmlns:pfx="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/UserSchema/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/UserSchema/Schema.xsd" schemaLocation="/SmartCity_Project/CommonModel/UserSchema/UserRequest.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/smatcity/SmartCity_Project/CommonModel/schema/JobseekersSchema/Schema.xsd" schemaLocation="/SmartCity_Project/CommonModel/AdminSchema/JobseekersSchema/JobsseekersReq.xsd"/>
    <pd:name>SmartCity_Project/USER_MODULE/JOBSEEKERS/main/Get_jobseekers_details.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:UserReq"/>
    <pd:startX>65</pd:startX>
    <pd:startY>116</pd:startY>
    <pd:returnBindings>
        <pfx2:JobseekersResp>
            <pfx2:Status>
                <xsl:value-of select="$Jobseekers/pfx2:JobseekersResp/pfx2:Status"/>
            </pfx2:Status>
            <pfx2:message>
                <xsl:value-of select="$Jobseekers/pfx2:JobseekersResp/pfx2:message"/>
            </pfx2:message>
            <xsl:for-each select="$Jobseekers/pfx2:JobseekersResp/pfx2:JobseekersInfo">
                <pfx2:JobseekersInfo>
                    <pfx2:JobId>
                        <xsl:value-of select="pfx2:JobId"/>
                    </pfx2:JobId>
                    <pfx2:Role>
                        <xsl:value-of select="pfx2:Role"/>
                    </pfx2:Role>
                    <pfx2:postedDate>
                        <xsl:value-of select="pfx2:postedDate"/>
                    </pfx2:postedDate>
                    <pfx2:Company>
                        <xsl:value-of select="pfx2:Company"/>
                    </pfx2:Company>
                    <pfx2:Industries>
                        <xsl:value-of select="pfx2:Industries"/>
                    </pfx2:Industries>
                    <pfx2:location>
                        <xsl:value-of select="pfx2:location"/>
                    </pfx2:location>
                    <pfx2:Experience>
                        <xsl:value-of select="pfx2:Experience"/>
                    </pfx2:Experience>
                    <pfx2:Job_Type>
                        <xsl:value-of select="pfx2:Job_Type"/>
                    </pfx2:Job_Type>
                    <pfx2:Job_description>
                        <xsl:value-of select="pfx2:Job_description"/>
                    </pfx2:Job_description>
                    <xsl:for-each select="pfx2:Requirements">
                        <pfx2:Requirements>
                            <pfx2:degree>
                                <xsl:value-of select="pfx2:degree"/>
                            </pfx2:degree>
                            <pfx2:skills>
                                <xsl:value-of select="pfx2:skills"/>
                            </pfx2:skills>
                            <pfx2:year_of_exp>
                                <xsl:value-of select="pfx2:year_of_exp"/>
                            </pfx2:year_of_exp>
                        </pfx2:Requirements>
                    </xsl:for-each>
                    <xsl:for-each select="pfx2:Contact_no">
                        <pfx2:Contact_no>
                            <pfx2:name>
                                <xsl:value-of select="pfx2:name"/>
                            </pfx2:name>
                            <pfx2:email>
                                <xsl:value-of select="pfx2:email"/>
                            </pfx2:email>
                            <pfx2:phone>
                                <xsl:value-of select="pfx2:phone"/>
                            </pfx2:phone>
                        </pfx2:Contact_no>
                    </xsl:for-each>
                </pfx2:JobseekersInfo>
            </xsl:for-each>
        </pfx2:JobseekersResp>
