        </Correlatn_Id>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1681755290431</pd:targetNamespace>
    <pd:activity name="Jobseekers">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>368</pd:x>
        <pd:y>117</pd:y>
        <config>
            <processName>/SmartCity_Project/USER_MODULE/JOBSEEKERS/main/subprocess/Jobseekers.process</processName>
        </config>
        <pd:inputBindings>
            <pfx:UserReq>
                <pfx:Search_criteria>
                    <xsl:value-of select="$Start/pfx:UserReq/pfx:Search_criteria"/>
                </pfx:Search_criteria>
            </pfx:UserReq>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="startlog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>254</pd:x>
        <pd:y>119</pd:y>
        <config>
            <processName>/Common_Framework_Sh/logexceptionproccess/commonlogs.process</processName>
        </config>
        <pd:inputBindings>
            <ns1:LogHandler>
                <ns1:ApplictionName>
                    <xsl:value-of select="&quot;business_details&quot;"/>
                </ns1:ApplictionName>
                <ns1:ProcessName>
                    <xsl:value-of select="&quot;Get_business_details&quot;"/>
                </ns1:ProcessName>
                <ns1:CorrelationID>
                    <xsl:value-of select="$Correlatn_Id/Correlatn_Id"/>
                </ns1:CorrelationID>
                <ns1:Status>
                    <xsl:value-of select="&quot;success&quot;"/>
                </ns1:Status>
                <ns1:LogType>
                    <xsl:value-of select="&quot;startlog&quot;"/>
                </ns1:LogType>
                <ns1:Current_Timestamp>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:Current_Timestamp>
                <ns1:Payload>
                    <xsl:value-of select="string($Start/pfx:UserReq/pfx:Search_criteria)"/>
                </ns1:Payload>
            </ns1:LogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>173</pd:x>
        <pd:y>118</pd:y>
        <config>
            <variableName>Correlatn_Id</variableName>
        </config>
        <pd:inputBindings>
            <Correlatn_Id>
                <xsl:value-of select="custom_func:getUUID()"/>
            </Correlatn_Id>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="end_log">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>482</pd:x>
        <pd:y>115</pd:y>
        <config>
            <processName>/Common_Framework_Sh/logexceptionproccess/commonlogs.process</processName>
        </config>
        <pd:inputBindings>
            <ns1:LogHandler>
                <xsl:if test="$startlog/ns1:LogHandler/ns1:ApplictionName">
                    <ns1:ApplictionName>
                        <xsl:value-of select="$startlog/ns1:LogHandler/ns1:ApplictionName"/>
                    </ns1:ApplictionName>
                </xsl:if>
                <xsl:if test="$startlog/ns1:LogHandler/ns1:ProcessName">
                    <ns1:ProcessName>
                        <xsl:value-of select="$startlog/ns1:LogHandler/ns1:ProcessName"/>
                    </ns1:ProcessName>
                </xsl:if>
                <xsl:if test="$startlog/ns1:LogHandler/ns1:CorrelationID">
                    <ns1:CorrelationID>
                        <xsl:value-of select="$startlog/ns1:LogHandler/ns1:CorrelationID"/>
                    </ns1:CorrelationID>
                </xsl:if>
                <xsl:if test="$startlog/ns1:LogHandler/ns1:Status">
                    <ns1:Status>
                        <xsl:value-of select="$startlog/ns1:LogHandler/ns1:Status"/>
                    </ns1:Status>
                </xsl:if>
                <ns1:LogType>
                    <xsl:value-of select="&quot;endlog&quot;"/>
                </ns1:LogType>
                <xsl:if test="$startlog/ns1:LogHandler/ns1:Current_Timestamp">
                    <ns1:Current_Timestamp>
                        <xsl:value-of select="$startlog/ns1:LogHandler/ns1:Current_Timestamp"/>
                    </ns1:Current_Timestamp>
                </xsl:if>
                <xsl:if test="$startlog/ns1:LogHandler/ns1:Payload">
                    <ns1:Payload>
                        <xsl:value-of select="$startlog/ns1:LogHandler/ns1:Payload"/>
                    </ns1:Payload>
                </xsl:if>
            </ns1:LogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>startlog</pd:from>
        <pd:to>Jobseekers</pd:to>
        <pd:xpathDescription>validate</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/pfx:UserReq/pfx:Search_criteria = 'Jobseekers'</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Assign</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign</pd:from>
        <pd:to>startlog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Jobseekers</pd:from>
        <pd:to>end_log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>end_log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>