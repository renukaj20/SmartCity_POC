<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>SmartCity_Project/ADMINISTRATION_MODULE/login/Wait_Process.process</pd:name>
    <pd:startName>Receive Notification</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="Receive Notification">
        <pd:type>com.tibco.plugin.waitnotify.WaitEventSource</pd:type>
        <pd:resourceType>ae.activities.waitStarter</pd:resourceType>
        <pd:x>113</pd:x>
        <pd:y>119</pd:y>
        <config>
            <sharedConfig>/SmartCity_Project/ADMINISTRATION_MODULE/Resource/Notify_Configuration.sharednotify</sharedConfig>
            <key>true</key>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>402</pd:endX>
    <pd:endY>112</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1684295432349</pd:targetNamespace>
    <pd:activity name="Write File">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>259</pd:x>
        <pd:y>113</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
        </config>
        <pd:inputBindings>
            <ns:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="&quot;C:\SMART_CITY_project\Required_DOC'S\Notification.txt&quot;"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="concat(&quot;Message is received : &quot;, $Receive-Notification/message,&quot;at&quot;,current-dateTime())"/>
                </textContent>
                <addLineSeparator>
                    <xsl:value-of select="true()"/>
                </addLineSeparator>
            </ns:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Write File</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Receive Notification</pd:from>
        <pd:to>Write File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>