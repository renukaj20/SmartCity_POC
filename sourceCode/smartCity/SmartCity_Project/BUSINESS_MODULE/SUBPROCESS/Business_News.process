                <pfx3:NewsResp>
                    <pfx3:status>
                        <xsl:value-of select="&quot;failed&quot;"/>
                    </pfx3:status>
                    <pfx3:message>
                        <xsl:value-of select="&quot;no records  found&quot;"/>
                    </pfx3:message>
                </pfx3:NewsResp>
            </xsl:otherwise>
        </xsl:choose>
