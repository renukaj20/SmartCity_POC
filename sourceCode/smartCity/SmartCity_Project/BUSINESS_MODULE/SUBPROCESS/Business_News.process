    <pd:endX>675</pd:endX>
    <pd:endY>168</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1680788987043</pd:targetNamespace>
    <pd:activity name="add_Business_News">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>441</pd:x>
        <pd:y>67</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>INSERT INTO SYSTEM.SCP_BUSINESSNEWS (TITLE,DESCRIPTION,PUBLISHED_DATE,SOURCE) VALUES(?,?,?,?)</statement>
            <jdbcSharedConfig>/SmartCity_Project/CommonModel/Resource/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>TITLE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>DESCRIPTION</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PUBLISHED_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>SOURCE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <TITLE>
                    <xsl:value-of select="$Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Title"/>
                </TITLE>
                <DESCRIPTION>
                    <xsl:value-of select="$Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Description"/>
                </DESCRIPTION>
                <PUBLISHED_DATE>
                    <xsl:value-of select="$Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Published_Date"/>
                </PUBLISHED_DATE>
                <SOURCE>
                    <xsl:value-of select="$Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Source"/>
                </SOURCE>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>109</pd:x>
        <pd:y>282</pd:y>
        <config>
            <faultName/>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="string-length($Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Title)=0">
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorMessages/titlemsg"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorCodes/titlecode"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:when>
                <xsl:when test="string-length($Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Description)=0">
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorMessages/descriptn-msg"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorCodes/descrptn_code"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:when>
                <xsl:when test="string-length($Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Published_Date)=0">
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorMessages/pub-dtmsg"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorCodes/pub-dtcode"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:when>
                <xsl:when test="not(custom_func:matchRegex($Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Published_Date,'^([0-9]{4})-([0-1][0-9])-([0-3][0-9])\s([0-1][0-9]|[2][0-3]):([0-5][0-9]):([0-5][0-9])$'))">
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorMessages/pub-properfrmt"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorCodes/pub-date-format-code"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:when>
                <xsl:otherwise>
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="&quot;no records found&quot;"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="&quot;105&quot;"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="fetch_Business_News">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>372</pd:x>
        <pd:y>160</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SmartCity_Project/CommonModel/Resource/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT   PUBLISHED_DATE  FROM   SCP_BUSINESSNEWS</statement>
            <QueryOutputCachedSchemaColumns>PUBLISHED_DATE</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <oraObjects/>
            <oraTables/>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Update_Business_News">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>518</pd:x>
        <pd:y>160</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SmartCity_Project/CommonModel/Resource/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>UPDATE  SCP_BUSINESSNEWS
  SET   TITLE = ?, DESCRIPTION = ? ,SOURCE = ? 
  WHERE    PUBLISHED_DATE = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>TITLE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>DESCRIPTION</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>SOURCE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PUBLISHED_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <TITLE>
                    <xsl:value-of select="$Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Title"/>
                </TITLE>
                <DESCRIPTION>
                    <xsl:value-of select="$Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Description"/>
                </DESCRIPTION>
                <SOURCE>
                    <xsl:value-of select="$Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Source"/>
                </SOURCE>
                <PUBLISHED_DATE>
                    <xsl:value-of select="$Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Published_Date"/>
                </PUBLISHED_DATE>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="delete_Business_News">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>447</pd:x>
        <pd:y>276</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>DELETE FROM SCP_BUSINESSNEWS    WHERE  PUBLISHED_DATE = ? </statement>
            <jdbcSharedConfig>/SmartCity_Project/CommonModel/Resource/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>PUBLISHED_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <PUBLISHED_DATE>
                    <xsl:value-of select="$Start/pfx3:NewsReq/pfx3:NewsInfo/pfx3:Published_Date"/>
                </PUBLISHED_DATE>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Null">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
