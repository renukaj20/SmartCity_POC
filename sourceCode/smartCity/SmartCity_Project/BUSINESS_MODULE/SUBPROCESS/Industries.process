                <parameter>
                    <parameterName>INDUSTRY_ID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <INDUSTRY_ID>
                    <xsl:choose>
                        <xsl:when test="exists($FETCH_Industries/resultSet/Record[1]/INDUSTRY_ID)">
                            <xsl:value-of select="$FETCH_Industries/resultSet/Record[1]/INDUSTRY_ID"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </INDUSTRY_ID>
                <INDUSTRY_NAME>
                    <xsl:value-of select="$Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Industry_name"/>
                </INDUSTRY_NAME>
                <DESCRIPTIONS>
                    <xsl:value-of select="$Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Descriptions"/>
                </DESCRIPTIONS>
                <LOCATIONS>
                    <xsl:value-of select="$Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Locations"/>
                </LOCATIONS>
                <CONTACT_EMAIL>
                    <xsl:value-of select="$Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Contact_email"/>
                </CONTACT_EMAIL>
                <INDUSTRY_ID>
                    <xsl:value-of select="$Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Industry_Id"/>
                </INDUSTRY_ID>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
