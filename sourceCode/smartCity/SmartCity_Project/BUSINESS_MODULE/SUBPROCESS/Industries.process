            <jdbcSharedConfig>/SmartCity_Project/CommonModel/Resource/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>UPDATE  SCP_INDUSTRIES
  SET INDUSTRY_ID = ?,INDUSTRY_NAME = ?,DESCRIPTIONS = ?,LOCATIONS = ? ,CONTACT_EMAIL =?
  WHERE INDUSTRY_ID = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>INDUSTRY_ID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>INDUSTRY_NAME</parameterName>
