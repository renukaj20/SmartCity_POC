    <pd:activity name="ADD_Industries">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>404</pd:x>
        <pd:y>45</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>INSERT INTO SYSTEM.SCP_INDUSTRIES (INDUSTRY_ID,INDUSTRY_NAME,DESCRIPTIONS,LOCATIONS,CONTACT_EMAIL) VALUES(?,?,?,?,?)</statement>
            <jdbcSharedConfig>/SmartCity_Project/CommonModel/Resource/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>INDUSTRY_ID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>INDUSTRY_NAME</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>DESCRIPTIONS</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>LOCATIONS</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>CONTACT_EMAIL</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <INDUSTRY_ID>
                    <xsl:value-of select="$Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Industry_Id"/>
                </INDUSTRY_ID>
                <INDUSTRY_NAME>
                    <xsl:value-of select="$Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Industry_name"/>
                </INDUSTRY_NAME>
                <DESCRIPTIONS>
                    <xsl:value-of select="$Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Descriptions"/>
                </DESCRIPTIONS>
                <LOCATIONS>
                    <xsl:value-of select="$Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Locations"/>
                </LOCATIONS>
                <CONTACT_EMAIL>
                    <xsl:value-of select="$Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Contact_email"/>
                </CONTACT_EMAIL>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>102</pd:x>
        <pd:y>258</pd:y>
        <config>
            <faultName/>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="not(custom_func:matchRegex($Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Industry_Id,'[A-Za-z][A-Za-z0-9_]{7,9}'))">
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorMessages/industry-idmsg"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorCodes/industry-idCode"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:when>
                <xsl:when test="string-length($Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Descriptions)=0">
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorMessages/descriptn-msg"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorCodes/descrptn_code"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:when>
                <xsl:when test="not(custom_func:matchRegex($Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Contact_email,'[a-zA-Z0-9+_.-]+@[a-zA-Z0-9.-]+[a-zA-Z]'))">
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorMessages/contact-mailmsg"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorCodes/contactmailCode"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:when>
                <xsl:when test="string-length($Start/pfx2:IndustriesReq/pfx2:IndustriesInfo/pfx2:Industry_name) =0">
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorMessages/industry-nameMsg"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorCodes/industry-nameCode"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:when>
                <xsl:otherwise>
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorMessages/No-records"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/validations/ErrorCodes/No-recordsCode"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
