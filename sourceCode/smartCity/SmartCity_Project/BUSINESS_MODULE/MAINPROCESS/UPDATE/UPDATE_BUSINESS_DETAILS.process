                                    <xsl:value-of select="ns3:Title"/>
                                </pfx:Title>
                                <pfx:Description>
                                    <xsl:value-of select="ns3:Description"/>
                                </pfx:Description>
                                <pfx:Published_Date>
                                    <xsl:value-of select="ns3:Published_Date"/>
                                </pfx:Published_Date>
                                <pfx:Source>
                                    <xsl:value-of select="ns3:Source"/>
                                </pfx:Source>
                            </pfx:News>
                        </xsl:for-each>
                    </pfx:BusinessInfo>
                </pfx:BusinessResp>
            </xsl:when>
            <xsl:when test="exists($Start/pfx:BusinessReq/pfx:BusinessInfo/pfx:Business_centers)">
                <pfx:BusinessResp>
                    <pfx:Status>
                        <xsl:value-of select="$Business_centers/ns2:Business_centersResp/ns2:status"/>
