                                </pfx:Industry_name>
                                <pfx:Descriptions>
                                    <xsl:value-of select="ns:IndustriesResp/ns:IndustriesInfo/ns:Descriptions"/>
                                </pfx:Descriptions>
                                <pfx:Locations>
                                    <xsl:value-of select="ns:IndustriesResp/ns:IndustriesInfo/ns:Locations"/>
                                </pfx:Locations>
                                <pfx:Contact_email>
                                    <xsl:value-of select="ns:IndustriesResp/ns:IndustriesInfo/ns:Contact_email"/>
                                </pfx:Contact_email>
                            </pfx:Industries>
                        </xsl:for-each>
                    </pfx:BusinessInfo>
                </pfx:BusinessResp>
            </xsl:when>
            <xsl:otherwise>
                <pfx:BusinessResp>
