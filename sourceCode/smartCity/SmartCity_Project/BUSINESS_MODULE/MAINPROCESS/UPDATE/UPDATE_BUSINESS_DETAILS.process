                                    <xsl:value-of select="ns2:Center_name"/>
                                </pfx:Center_name>
                                <pfx:Locations>
                                    <xsl:value-of select="ns2:Locations"/>
                                </pfx:Locations>
                                <pfx:Descriptions>
                                    <xsl:value-of select="ns2:Descriptions"/>
                                </pfx:Descriptions>
                                <pfx:Contact_email>
                                    <xsl:value-of select="ns2:Contact_email"/>
                                </pfx:Contact_email>
                            </pfx:Business_centers>
                        </xsl:for-each>
                    </pfx:BusinessInfo>
                </pfx:BusinessResp>
            </xsl:when>
            <xsl:when test="exists($Start/pfx:BusinessReq/pfx:BusinessInfo/pfx:Industries)">
                <pfx:BusinessResp>
                    <pfx:Status>
                        <xsl:value-of select="$Industries/ns:IndustriesResp/ns:status"/>
