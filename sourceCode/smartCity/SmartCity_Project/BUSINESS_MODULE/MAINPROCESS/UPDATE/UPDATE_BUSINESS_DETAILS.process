                        <xsl:value-of select="&quot;no records found&quot;"/>
                    </pfx:Status>
                    <pfx:message>
                        <xsl:value-of select="&quot;no records found&quot;"/>
                    </pfx:message>
                </pfx:BusinessResp>
            </xsl:otherwise>
