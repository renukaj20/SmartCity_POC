            <BadgerfishCheck>false</BadgerfishCheck>
            <removeRootElementForInput>true</removeRootElementForInput>
            <ActivityInputEditor ref="pfx3:StudentDetailsResp"/>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInputClass>
                <pfx3:StudentDetailsResp>
                    <pfx3:Status>
                        <xsl:value-of select="$UPDATE_STUDENT_DETAILS/pfx3:StudentDetailsResp/pfx3:Status"/>
                    </pfx3:Status>
                    <pfx3:Message>
                        <xsl:value-of select="$UPDATE_STUDENT_DETAILS/pfx3:StudentDetailsResp/pfx3:Message"/>
                    </pfx3:Message>
                    <pfx3:StudentDetails>
                        <xsl:for-each select="$UPDATE_STUDENT_DETAILS/pfx3:StudentDetailsResp/pfx3:StudentDetails/pfx3:Edu.Institutes">
