        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>379</pd:x>
        <pd:y>82</pd:y>
        <config>
            <processName>/SmartCity_Project/STUDENT_MODULE/MAINPROCESS/UPDATE/UPDATE_STUDENT_DETAILS.process</processName>
        </config>
        <pd:inputBindings>
            <pfx3:StudentetailsReq>
                <xsl:if test="$JSONReq/ns2:ActivityOutputClass/pfx3:StudentetailsReq/pfx3:operations">
                    <pfx3:operations>
                        <xsl:value-of select="$JSONReq/ns2:ActivityOutputClass/pfx3:StudentetailsReq/pfx3:operations"/>
                    </pfx3:operations>
                </xsl:if>
                <pfx3:StudentetailsInfo>
                    <xsl:for-each select="$JSONReq/ns2:ActivityOutputClass/pfx3:StudentetailsReq/pfx3:StudentetailsInfo/pfx3:Edu.Institutes">
