                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx8:LibrariesReq/pfx8:LibrariesInfo[1]/pfx8:lib_name)">
                            <xsl:value-of select="$Start/pfx8:LibrariesReq/pfx8:LibrariesInfo[1]/pfx8:lib_name"/>
